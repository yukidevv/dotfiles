Config {font="Ricty Diminished"
       ,bgColor = "#363434"
       ,fgColor = "#787576"
       , commands =
               [
               Run Date "<fc=#f54272>[%a] %m/%d %H:%M</fc>" "date" 10
               ,Run StdinReader
               ,Run DynNetwork     [ "--template" , "<fc=#4287f5><dev>: ↑<tx>kB/s|↓<rx>kB/s</fc>"
                             , "--Low"      , "1000"       -- units: kB/s
                             , "--High"     , "5000"       -- units: kB/s
                             , "--low"      , "darkgreen"
                             , "--normal"   , "darkorange"
                             , "--high"     , "darkred"
                             ] 10
               ]
       , sepChar = "%"
       , alignSep = "}{"
       ,template = "%StdinReader% }{%dynnetwork%|<action=`flameshot gui`><fc=#42f599>Prt</fc></action>|<action=`~/bin/gdrive.sh mount`><action=`~/bin/gdrive.sh` button=3><fc=#42f599>G</fc></action></action>|<action=`xclock`>%date%</action>"
}

-- 配色設定
--bgColor = "#363434"
--cBlue = "#4287f5"
--cRed = "#f54272"
--cGreen = "#42f599"
--cYellow = "#f5d142"
--cPink = "#f542bf"
--cOrange = "#f58a42"
--cGrey = "#c2c2c2"
--cDarkGrey = "#787576"
